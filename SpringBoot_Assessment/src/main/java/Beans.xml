<?xml version = "1.0" encoding = "UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
     http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
     http://www.springframework.org/schema/context
     http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<context:annotation-config />
	<!-- Definition for employee bean -->
	<bean id="employee" class="q1.com.example.demo.entity.Employee">
		<property name="empId" value="1" />
		<property name="empName" value="Apple" />
		<property name="empSal" value="11.00" />
	</bean>
		<bean id="employee2" class="q1.com.example.demo.entity.Employee">
		<property name="empId" value="2" />
		<property name="empName" value="Banana" />
		<property name="empSal" value="99.00" />
	</bean>
</beans>